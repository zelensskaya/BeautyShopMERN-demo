{
  "name": "beautyshop",
  "version": "2.0.0",
  "description": "eCommerce apllication built with the MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zelensskaya/BeautyShopMERN-demo.git"
  },
  "author": "Maryna Zelenska",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zelensskaya/BeautyShopMERN-demo/issues"
  },
  "homepage": "https://github.com/zelensskaya/BeautyShopMERN-demo#readme",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.4",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "currently": "^0.0.8",
    "dotenv": "^17.2.3",
    "nodemon": "^3.1.11"
  }
}
